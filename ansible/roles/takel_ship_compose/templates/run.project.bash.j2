{{ takel_ship_scripts_shebang }}

docker compose \
{% for project in takel_ship_compose_projects %}
{% if project['name'] == takel_ship_compose_project['name'] %}
{% for service in project['services'] %}
  --env-file ../{{ service['name'] }}/env \
{% endfor %}
  --file {{
      takel_ship_compose_service_filename }}.{{ project['name'] }}.{{
      takel_ship_compose_service_fileext }} \
{% endif %}
{% endfor %}
  up -d
